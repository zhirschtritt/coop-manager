{
  "name": "bike-coop-manager",
  "version": "1.0.0",
  "description": "Membership coop manager service",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "build:server": "turbo run build --scope=@bikecoop/server",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prune:web": "turbo prune --scope=@bikecoop/web --docker",
    "prune:server": "turbo prune --scope=@bikecoop/server --docker"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.8.3",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
