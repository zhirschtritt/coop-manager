{
  "name": "bike-coop-manager",
  "version": "1.0.0",
  "description": "Membership coop manager service",
  "scripts": {
    "clean": "rimraf \"packages/**/lib\" \"packages/**/*.tsbuildinfo\"",
    "build": "tsc -b tsconfig.build.json",
    "bootstrap": "npx lerna bootstrap",
    "test": "npx lerna run test",
    "version": "npx lerna version",
    "build:common": "npx lerna run build --scope @bikecoop/common"
  },
  "devDependencies": {
    "lerna": "5.6.2",
    "rimraf": "3.0.2",
    "typescript": "4.8.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
