    "eslint": "^7.32.0",
    "jest": "26.6.0",
    "jest-junit": "^12.3.0",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.4.0",
