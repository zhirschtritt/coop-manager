    "@types/cron": "^1.7.3",
    "@types/express": "4.17.13",
    "@types/hapi__joi": "17.1.7",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.171",
    "@types/node": "^14.18.36",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
