    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.8.2"
