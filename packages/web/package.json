    "@mantine/next": "5.2.5",
    "@mantine/notifications": "5.2.5",
    "@mantine/prism": "5.2.5",
    "@next/bundle-analyzer": "^12.0.7",
    "apollo-env": "^0.10.0",
    "apollo-server": "2.25.2",
    "apollo-server-express": "2.x.x",
    "axios": "^0.21.1",
    "date-fns": "^2.22.1",
